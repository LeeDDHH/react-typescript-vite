{
  "name": "react-typescript-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx src --fix",
    "lint:css": "stylelint ./src/**/*.{css,js,jsx,ts,tsx}",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "sb": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.26.3",
    "babel-loader": "^9.1.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "jsdom": "^21.0.0",
    "lefthook": "^1.2.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.2",
    "styled-components": "^5.3.6",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vitest": "^0.27.1"
  },
  "resolutions": {
    "stylelint/postcss": "8.4.14"
  }
}
